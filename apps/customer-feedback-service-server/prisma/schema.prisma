datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Feedback {
  comment    String?
  createdAt  DateTime  @default(now())
  customerId String?
  date       DateTime?
  id         String    @id @default(cuid())
  rating     Int?
  updatedAt  DateTime  @updatedAt
}
