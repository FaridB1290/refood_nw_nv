datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model InventoryItem {
  createdAt   DateTime  @default(now())
  id          String    @id @default(cuid())
  name        String?
  price       Float?
  quantity    Int?
  restockDate DateTime?
  supplier    String?
  updatedAt   DateTime  @updatedAt
}
